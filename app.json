{
  "expo": {
    "name": "MIXZTER",
    "slug": "musikquiz",
    "scheme": "musikquiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mixzter-icon-1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/mixzter-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0b0c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mixzter-adaptive-foreground.png",
        "backgroundColor": "#0b0b0c"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.clyd3frog.musikquiz",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "musikquiz-app.web.app",
              "pathPrefix": "/oauth2redirect/google"
            }
          ]
        },
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "musikquiz",
              "host": "oauthredirect"
            }
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bccb9723-b429-4f50-9167-dfc8c9c8d83c"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/bccb9723-b429-4f50-9167-dfc8c9c8d83c"
    },
    "plugins": [
      "expo-web-browser",
      "expo-audio"
    ]
  }
}
